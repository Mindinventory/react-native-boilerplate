{
  "name": "MIBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "fix:lint": "yarn lint --fix && yarn typescript",
    "generate:icons": "cd scripts && node icons.js",
    "generate:images": "cd scripts && node images.js",
    "generate:screen": "cd scripts && node generateScreenFile.js",
    "preios": "ENVFILE=.env",
    "preandroid": "ENVFILE=.env",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "postinstall": "npx pod-install",
    "prepare": "husky install",
    "refresh": "bash ./scripts/refresh.sh",
    "start": "react-native start",
    "test": "jest",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^9.3.8",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/native-stack": "^6.9.9",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.4",
    "i18n-js": "^4.2.2",
    "react": "18.2.0",
    "react-native": "0.71.7",
    "react-native-config": "^1.5.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-localize": "^2.2.6",
    "react-native-mmkv": "^2.6.3",
    "react-native-network-logger": "^1.14.1",
    "react-native-reanimated": "^3.1.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/i18n-js": "^3.8.4",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-order-autofix": "^0.8.3",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "@types/react": "^18.0.24"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
